{
  "expo": {
    "name": "CleanIndia",
    "slug": "CleanIndia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vinay_ambatkar.CleanIndia"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "82a8bea3-80f9-44b8-89b1-07b017a74f62"
      }
    },
    "owner": "rj729",
    "plugins": [
      "expo-build-properties",
      {
        "android": {
          "compileSdkVersion": 50.0.0,
          "targetSdkVersion": 50.0.0,
          "buildToolsVersion": "31.0.0"
        }
      }
    ]
  }
}
